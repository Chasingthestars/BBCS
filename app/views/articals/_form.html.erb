<%= simple_form_for(@artical) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :category %>
    <%= f.input :content %>
    <%= f.input :user %>
    <%= f.input :read_number %>
    <%= f.input :thumber_up_number %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
